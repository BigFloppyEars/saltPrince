doctype html
html(lang="en")
	head
		meta(name="viewport" content="width=device-width, initial-scale=1.0")
		link(href="styles/laststand-styles.css" rel="stylesheet" type="text/css")
		link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous")
	body
	.container-fluid
		.row
			.col-md-16
				h1(id="title") Last Stand
		#UI.row
			.col-md-4
				p(id="response") server reponse test
			#stop.col-md-4
				button stop
			#start.col-md-4
				button start
			#health.col-md-4
				p Player Health
				progress(value="0" max="100")
		.row
			.col-md-16
				canvas(id="myCanvas" width="0" height="0")

		img(id="spriteSheet" src="fullSheet100.png")
	footer
		script(src="https://code.jquery.com/jquery-2.2.4.js" integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI=" crossorigin="anonymous")
		script(data-main="js/laststand.js" src="js/require.js")
	